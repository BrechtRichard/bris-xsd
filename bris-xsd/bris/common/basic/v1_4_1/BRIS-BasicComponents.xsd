	<!-- Added with 3.0 messaging model -->
	<xsd:complexType name="RegistrationDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Registration date (of a company or a branch)
				Data.
				Type: DateType
				Content: Date
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="StrikingOffDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Date of striking off (of a company or a branch)
				Data.
				Type: DateType
				Content: Date
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="UpdateDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Update date (related to some update of some data - generic approach)
				Data.
				Type: DateType
				Content: Date
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="BirthDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Birth date (of a natural person)
				Data.
				Type: DateType
				Content: Date
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DocumentTitleType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Title of a document
				DataType: NameType
				Content: String
				Examples: User's manual
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:InputNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="1000" />
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="PersonSurnameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Surname of a person
				DataType: NameType
				Content: String
				Examples: John
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:InputNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="1000" />
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="PersonNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of a person
				DataType: NameType
				Content: String
				Examples: Doe
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:InputNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="1000" />
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="PredefinedStatusType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value describing a predefined status for a company.
				DataType: CodeType
				Content: String
				Examples: CLOSED
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="CLOSED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The company is closed
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="STRUCK_OFF_THE_REGISTER">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The company has been struck off the register</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="WOUND_UP">
					<xsd:annotation>
						<!-- FIXME -->
						<xsd:documentation xml:lang="en">TO BE DEFINED</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="DISSOLVED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The company has been dissolved</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="ECONOMICALLY_ACTIVE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The company is economically active</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="ECONOMICALLY_INACTIVE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The company is economically inactive</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="PositionStatusType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value describing a status of a position.
				DataType: CodeType
				Content: String
				Examples: APPOINTMENT
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="APPOINTMENT">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">A new position</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="UPDATE_TO_EXISTING_APPOINTMENT">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The position changed</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Termination of a position</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:simpleType name="RepresentationType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value describing a representation of a person (be it a natural or legal one) on behalf of a company.
				DataType: CodeType
				Content: String
				Examples: ALONE
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ALONE">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The person alone is representing the company.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="JOINT">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The person is part of a joint representation of a company.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OTHER">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The person is part of another kind of representation.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="CompanyWebsiteURIType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Unique Resource Identifier of a company website
				DataType: IdentifierType
				Content: string
				Examples: https://www.iso.org/
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="URI" type="URIType"/>
			<xsd:element name="Fee" type="FeeType"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CompanyObjectDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of the object of a company, in textual form.
				DataType: TextType
				Content: string
				Examples: The company sells tools, machinery and supplies destined for woodworking.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Description" type="DescriptionType"/>
			<xsd:element name="Fee" type="FeeType"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CategoryCodeType">
		<xsd:simpleContent>
			<xsd:extension base="udt:TextType">
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="LimitedDescriptionType">
		<xsd:simpleContent>
			<xsd:restriction base="udt:TextType">
				<xsd:minLength value="3"/>
				<xsd:pattern value=".*[\S].*"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="DescriptionType">
		<xsd:simpleContent>
			<xsd:extension base="udt:TextType">
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="RepresentationDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of the representation a natural or legal person has towards a company
				DataType: TextType
				Content: string
				Examples: M. Doe is the single representative of the company
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="udt:TextType"/>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="UserRoleType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: User role, in textual form.
				DataType: TextType
				Content: string
				Examples: VIP, USER, ADMIN, ...
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="udt:TextType">
				<xsd:pattern value="[\S]{3,100}"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<!-- Introduced in version 4.0.0 of the BRIS Messaging Model -->
	<xsd:complexType name="HasCrossBorderDetailsType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: a boolean indicating if a company has cross-border operation(s) linked to it
				DataType: IndicatorType
				Content: Boolean
				Examples: 1, 0, true, false
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IndicationIndicatorType" />
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RegisterDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Register description, in textual form.
				DataType: TextType
				Content: string
				Examples: Register in which the document refer to article 14 are filled-ind
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="udt:TextType">
				<xsd:pattern value=".*[\S].*"/>
				<xsd:minLength value="3"/>
				<xsd:maxLength value="200"/>
			</xsd:restriction>
