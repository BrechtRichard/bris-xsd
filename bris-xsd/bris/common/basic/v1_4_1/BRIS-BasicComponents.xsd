		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code describing the operation to be performed for a
				specific
				subscription in the ECP.
				DataType: CodeType
				Content: String
				Examples: Add
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:AdjustmentReasonCodeType">
				<xsd:enumeration value="ADD">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Add a company to the
							subscription list of the issuing business register
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="REMOVE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Remove a company
							from the subscription list of the issuing business register
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ManageSubscriptionStatusCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code describing the operation to be performed for a
				specific
				subscription in the ECP.
				DataType: CodeType
				Content: String
				Examples: Add
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="PROCESSED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update in the
							subscription list has been successfully performed
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="IGNORED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update has not
							been perform because the request was out-dated
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
