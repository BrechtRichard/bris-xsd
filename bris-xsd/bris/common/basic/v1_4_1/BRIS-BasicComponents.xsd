					<xsd:documentation xml:lang="en">Finnish
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="fr">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">French
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ga">
