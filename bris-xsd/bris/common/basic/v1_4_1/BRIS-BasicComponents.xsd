	<xsd:complexType name="SizeLimitedDocumentIDType">
		<xsd:simpleContent>
			<xsd:restriction base="cbc:DocumentIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

    <xsd:complexType name="AckIndicatorType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Indicator for technical ack
				DataType: IndicatorType
				Examples: 1 (accepted)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:AcceptedIndicatorType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AddressLine1Type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Registered address of the company
				DataType: TextType
				Content: String
				Examples: 12, Bvd. de Pigalle, 75001 Paris, France
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LineType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AddressLine2Type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Registered address of the company
				DataType: TextType
				Content: String
				Examples: 12, Bvd. de Pigalle, 75001 Paris, France
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LineType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AddressLine3Type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Registered address of the company
				DataType: TextType
				Content: String
				Examples: 12, Bvd. de Pigalle, 75001 Paris, France
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LineType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AttachmentReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Indicator for technical ack
				DataType: IndicatorType
				Examples: 1 (accepted)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="BusinessRegisterIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Element making it possible to identify the domestic
				register of
				origin of a company
				DataType: IdentifierType
				Content:
				String
				Examples: RMC
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:RegistrationIDType">
				<xsd:minLength value="2" />
				<xsd:maxLength value="15" />
				<xsd:pattern value="[\S]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<!-- Starting with v2.0 this is a replacement of the old BusinessRegisterIDType element -->
	<xsd:complexType name="BusinessRegisterCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Element making it possible to label the domestic
				register of	origin of a company (unicity is not guaranteed)
				DataType: CodeType
				Content:
				String
				Examples: RMC
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IdentificationCodeType">
				<xsd:minLength value="2" />
				<xsd:maxLength value="15" />
				<xsd:pattern value="[\S]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="BusinessRegisterNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the business register where a company is
				registered
				DataType: NameType
				Content: String
				Examples: Registro Mercantil Central
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:NameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="128" />
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LocalisedBusinessRegisterNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the business register in original language
				DataType: NameType
				Content: String
				Examples: Companies House
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
            <!-- The extension is needed to create a new type name that will insist on the "localisation",
                 i.e. the languageID attribute of the base type will actually be used -->
			<xsd:extension base="BusinessRegisterNameType"/>
		</xsd:simpleContent>
	</xsd:complexType>
    <xsd:complexType name="EnglishBusinessRegisterNameType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                ComponentType: Basic
                Definition: Name of the business register where a company is
                registered
                DataType: NameType
                Content: String
                Examples: Registro Mercantil Central
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cbc:NameType">
                <xsd:minLength value="1" />
                <xsd:maxLength value="128" />
                <xsd:pattern value=".*[\S].*" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="CityType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the city of the registered office
				DataType:
				TextType
				Content: String
				Examples: Paris
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:CityNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="255" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
    <xsd:complexType name="AlternateIDType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                ComponentType: Basic
                Definition: Alternate identifier for a company. Typically, alternate
                IDs are
                used by some Member States participating in pilots.
                DataType: IdentifierType
                Content: String
                Examples: XXXX111
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cbc:CompanyIDType">
                <xsd:minLength value="1" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="CompanyAlternateIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Alternate identifier for a company. Typically, alternate
				IDs are
				used by some Member States participating in pilots.
				Data.Type: IdentifierType
				Content: String
				Examples: XXXX111
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:CompanyIDType">
				<xsd:minLength value="1" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyEUIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: The European Unique Identifier is a unique identifier of a company
				for the purpose of communication between registers in the context of BRIS.
				DataType: IdentifierType
				Content: String
				Examples:
				FRIG.2010012341-Z
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:CompanyIDType">
                <!--
                    NOTE: this pattern cannot enforce the length range of the registration number part of the EUID
                          This cannot be implemented in a readable way in XML Schema 1.0.
                          This validation would need to be delegated to business validation
                -->
				<xsd:pattern value="[A-Z]{2}[\S]{2,15}\.[\S]{3,35}" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyItemValidityStartDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Applicability start date of an item
				DataType: DateType
				Content: Date
				Examples:2016-01-10
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyItemValidityEndDateType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Applicability end date of an item
				DataType: DateType
				Content: Date
				Examples:2016-01-10
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyLegalFormType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Legal form of the company
				DataType: CodeType
				Content:
				String
				Examples: Societe par action simplifiee , S.A.S
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityCompanyTypeCodeType">
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the company
				DataType: NameType
				Content: String
				Examples: GDF Suez
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:InputNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="1000" />
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CompanyNameLanguageSpecificType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the company
				DataType: NameType
				Content: String
				Examples: GDF Suez
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityLegalNameType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="1000" />
				<xsd:pattern value=".*[\S].*" />
				<xsd:attribute name="languageID" type="LanguageCodeType"
					use="required" />					
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

    <xsd:complexType name="LocalisedCompanyNameType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                ComponentType: Basic
                Definition: Name of the company
                DataType: NameType
                Content: String
                Examples: GDF Suez
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cvb:LegalEntityLegalNameType">
                <xsd:minLength value="1" />
                <xsd:maxLength value="1000" />
                <xsd:pattern value=".*[\S].*" />
                <xsd:attribute name="languageID" type="LanguageCodeType"
                               use="required" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="QualifiedCompanyNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Name of the company
				DataType: NameType
				Content: String
				Examples: GDF Suez
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="LocalisedCompanyNameType">
				<xsd:attribute name="qualifierValue" type="CompanyNameQualifierValueType" default="main"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

    <xsd:complexType name="QualifiedEntityNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: A qualified entity name (with a qualifier and localization)
				DataType: NameType
				Content: String
				Examples: Whatever Trust
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="LocalisedCompanyNameType">
				<xsd:attribute name="qualifierValue" type="CompanyNameQualifierValueType" default="main"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:simpleType name="CompanyNameQualifierValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value identifying a type of company name
				DataType: CodeType
				Content:
				String
				Examples: Main
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="main"/>
			<xsd:enumeration value="secondary"/>
			<xsd:enumeration value="parallel"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:complexType name="CompanyRegistrationNumberType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Company number refers to the registration number of the
				company in
				the domestic register of origin
				DataType: IdentifierType
				Content: String
				Examples:ATU99999999
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityLegalIDType">
				<xsd:minLength value="3" />
				<xsd:maxLength value="35" />
				<xsd:pattern value="[\S]+" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CorrelationIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Reference of the message
				DataType: IDType
				Content:
				String
				Examples: normalizedString
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="CountryType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Country code for companies or business registers.
				According to the
				EU Publication Office [5], country code list uses
				ISO3166-1 alfa-2.
				DataType: CodeType
				Content: String
				Examples: BE
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IdentificationCodeType">
				<xsd:enumeration value="AT">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Austria
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="BE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Belgium
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="BG">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Bulgaria
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="CY">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Cyprus
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="CZ">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Czech Republic
						</xsd:documentation>
