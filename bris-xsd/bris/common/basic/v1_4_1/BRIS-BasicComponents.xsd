	<xsd:simpleType name="MergerTypeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value describing the type of merger that's happening
				DataType: CodeType
				Content: String
				Examples: WHOLLY_OWNED
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ACQUISITION_ART119_2A">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Cross-border merger by acquisition under article 119 (2) point a.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ACQUISITION_ART119_2D">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Cross-border merger by acquisition under article 119 (2) point d.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FORMATION">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Cross-border merger by formation of a new company under article 119 (2) point b.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WHOLLY_OWNED">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Cross-border merger of wholly owned company under article 119 (2) point c.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="DivisionTypeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
