	<xsd:complexType name="ParticularDateValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is a date
				Data.
				Type: DateType
				Content: Date
				Examples: 2017-12-12
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DateType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularDecimalValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is a decimal
				DataType: QuantityType
				Content: decimal
				Examples: 10,00
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:QuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularIntegerValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is an integer
				DataType: QuantityType
				Content: int
				Examples: 23
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:QuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ParticularStringValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value of the item if the required format is a string
				DataType: TextType
				Content: string
				Examples: Retail
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:TextType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="2048" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FaultErrorCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code identifying an error
				DataType: CodeType
				Content: String
				Examples: PORT0001E
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ResponseCodeType">
				<xsd:pattern value="[\S]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FaultErrorDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of an error.
				DataType: TextType
				Content: String
				Examples: Portal got a reply with no result.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:DescriptionType">
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FaultErrorSourceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: System in error.
				DataType: IdentifierType
				Content: String
				Examples: BR-BE-01
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ReferenceIDType" />
		</xsd:complexContent>
	</xsd:complexType>
    <xsd:complexType name="LegalFormAbbreviationType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                ComponentType: Basic
                Definition: Abbreviation used to referer to a given legal form
                DataType: TextType
                Content: String
                Example: Ltd
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cbc:NameCodeType">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="130"/>
                <xsd:pattern value=".*[\S].*" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="LegalFormCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code identifying a legal form of a company as defined by
				Article 1 of Directive 2009/101/EC
				DataType: CodeType
				Content: String
				Examples: LF_UK_004
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:LegalEntityCompanyTypeCodeType">
				<xsd:minLength value="9" />
				<xsd:pattern value="[\S]*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LegalFormCodeCountryType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Country of origin of the legal form code list
				DataType: CodeType
				Content: String
				Examples: UK
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IdentificationCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
    <!-- NOT USED
    <xsd:complexType name="LegalFormCodeValueType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                ComponentType: Basic
                Definition: Code of the legal form
                DataType: NameType
                Content: String
                Examples: LF_UK_004
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="cvb:LegalEntityLegalNameType" />
        </xsd:complexContent>
    </xsd:complexType>
    -->
	<xsd:complexType name="LegalFormDescriptionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of a legal form
				DataType: TextType
				Content:
				String
				Example: A private company with liability limited by the amount of unpaid
                capital on shares. Unable to make shares availble on the stock exchange.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:DescriptionType">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="1024"/>
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LegalFormLanguageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Language of the legal form description
				DataType:
				CodeType
				Content: String
				Examples: bg
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:EncodingCodeType" />
		</xsd:complexContent>
	</xsd:complexType>
    <xsd:complexType name="LegalFormRemarksType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                ComponentType: Basic
                Definition: Remarks associated with a legal form
                DataType: TextType
                Content:
                String
                Example: Includes Welsh equivalent Cyfyngedig - cyf
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cbc:RemarksType">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="1024"/>
                <xsd:pattern value=".*[\S].*" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="LegalFormTitleType">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                ComponentType: Basic
                Definition: Title of a legal form
                DataType: TextType
                Content:
                String
                Example: Private Limited
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cbc:TitleType">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="1024"/>
                <xsd:pattern value=".*[\S].*" />
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="LegislationReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Relevant national or EU legislation.
				DataType: TextType
				Content: String
				Examples: Article 13 of Directive 2005/56/EC, as
				amended by Directive
				2012/17/EU (Art. 2)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:LegalReferenceType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="MergerType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code of a merger as defined by Article 2(2) of Directive
				2005/56/EC
				[5].
				DataType: CodeType
				Content: String
				Examples:
				Acquisition
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:NotificationTypeCodeType">
				<xsd:enumeration value="ACQUISITION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Cross-border merger
							by acquisition
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="FORMATION_OF_NEW_COMPANY">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Cross-border merger
							by formation of new company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="OWNED_COMPANY">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Cross-border merger
							of wholly owned company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="MessageIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Reference of the message
				DataType: IDType
				Content:
				String
				Examples: normalizedString
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="NumberOfResultsInMessageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Number of results in message.
				DataType: QuantityType
				Content: int
				Examples: 4
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DeliveredQuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="NumberOfResultsInRegistersType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Total number of results found in register(s).
				DataType:
				QuantityType
				Content: int
				Examples: 5
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:DeliveredQuantityType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PaymentReferenceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Reference of the payment for one or several documents
				belonging to
				a specific Business Register.
				DataType: TextType
				Content: String
				Examples: 31234567
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ReferenceType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="PostalCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Postal code
				DataType: TextType
				Content: String
				Examples:
				4000
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:PostalZoneType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="10" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ProceedingType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code of a branch disclosure event as given in Article 5a
				(1) of
				Directive 89/666/EEC [6].
				DataType: CodeType
				Content: String
				Examples: WindingUpOpening
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:NotificationTypeCodeType">
				<xsd:enumeration value="WINDING_UP_OPENING">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up Opening
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="WINDING_UP_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up
							Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="WINDING_UP_OPENING_AND_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up Opening
							and Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="WINDING_UP_REVOCATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Winding-up
							Revocation
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_OPENING">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency Opening
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency
							Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_OPENING_AND_TERMINATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency Opening
							and Termination
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="INSOLVENCY_REVOCATION">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Insolvency
							Revocation
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="STRIKING_OFF">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Striking-off
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="NO_STRIKING_OFF_OF_BRANCH">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Article 5a(5) – No-Striking-off-of Branch
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

