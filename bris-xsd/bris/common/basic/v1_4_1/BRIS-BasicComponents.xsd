				<xsd:annotation>
					<xsd:documentation xml:lang="en">Danish
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="de">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">German
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="el">
