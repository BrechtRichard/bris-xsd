		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FULL_DIVISION">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Cross-border full division under point a of article 160b (4).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PARTIAL_DIVISION">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Cross-border partial division under point b of article 160b (4).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DIVISION_BY_SEPARATION">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Cross-border division by separation under point c of article 160b (4).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>

