	<xsd:complexType name="NationalPersonalIdentificationNumberType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: National Personal Identification Number
				DataType: TextType
				Content: String
				Examples: 001/021545125/555-R
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cvb:PersonIDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="IdentificationDocumentNumberType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Identification Document Number
				DataType: TextType
				Content: String
				Examples: 001/021545125/555-R
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="SizeLimitedDocumentIDType">
				<xsd:attribute name="belongsTo" type="IdentificationDocumentType" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="NationalityType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Nationality
				DataType: TextType
				Content: String
				Examples: French
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:NationalityIDType"> <!-- TODO verify the definition makes sense -->
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="BirthPlaceNameType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Nationality
				DataType: TextType
				Content: String
				Examples: French
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:BirthplaceNameType"> <!-- TODO verify the definition makes sense -->
				<xsd:minLength value="1" />
				<xsd:maxLength value="64" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:simpleType name="FinancialPeriodType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Financial period like YEAR or MONTH OF YEAR
				DataType: TextType
				Content: String
				Examples: YEAR
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1" />
			<xsd:maxLength value="64" />
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:simpleType name="IdentificationDocumentType">
		<!-- TODO define the type with an enumeration or use a string and business rule -->
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Type of an identification document
				DataType: TextType
				Content: String
				Examples: Passport
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1" />
			<xsd:maxLength value="64" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="RelevanceType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value indicating how much a result matches the search
				criteria.
				DataType: NumericType
				Content: int
				Examples: 2
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:WeightNumericType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ProcessingNodeNameIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Specifies the name of the node where the
				BR-PingRequest has been processed.
				Data.
				Type: IdentifierType
				Content: String
				Examples: XXXX111
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IDType">
				<xsd:minLength value="1" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ProcessingTimeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Specifies a time duration in milliseconds
				Examples: 600001
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:TimeAmountType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SubscriptionStatusCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code describing the status for a specific subscription
				in the ECP.
				DataType: CodeType
				Content: String
				Examples: Subscribed
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="SUBSCRIBED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The business
							register is in the subscription list for a specific parent
							company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="NOT_SUBSCRIBED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The business
							register is not in the subscription list for a specific parent
							company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="SubscriptionStatusValueType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Value describing the status for a specific subscription
				in the ECP.
				DataType: CodeType
				Content: String
				Examples: Subscribed
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="SUBSCRIBED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The business
							register is in the subscription list for a specific parent
							company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="NOT_SUBSCRIBED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The business
							register is not in the subscription list for a specific parent
							company
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="SUBSCRIBED_PARENT_EUID_NOT_FOUND_IN_THE_LED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">
							The business register is in the subscription list for a specific parent
							company BUT the parent EUID was not found in the list of known EUIDs
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="TestCaseIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test Case identifier
				DataType:IdentifierType
				Examples: TC-FN-009-08
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestConditionIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test execution condition identifier
				Data.
				Type:IdentifierType
				Examples:1
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestExecutionIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test Execution identifier.
				Represents that actual sending of a single message
				in the context of a test
				session
				DataType:IdentifierType
				Examples:
				123456
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ReferenceIDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestPackageIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test package identifier
				DataType:IdentifierType
				Examples:TPK-FN-BDN
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:IDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TestSessionIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Test Session identifier.
				unique identifier that correlates a specific test case
				with a group of test cases.
				DataType:IdentifierType
				Examples:
				123456
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:ReferenceIDType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TimeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Time when the message was sent.To specify a time zone,
				enter a time
				in UTC time by adding a "Z" behind the time
				DataType:
				TimeType
				Examples: 12:00:00Z
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:EffectiveTimeType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TimeAmountType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Specifies a time duration in milliseconds
				Examples: 600001
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="cbc:TimeAmountType" />
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="UpdateLEDCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Code used to define the type of update to be performed
				in the LED.
				DataType: CodeType
				Content: String
				Examples: Update
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:AdjustmentReasonCodeType">
				<xsd:enumeration value="UPDATE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Update of company
							data
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="DELETE">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">Delete of company
							data
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="UpdateLEDUpdateIDType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: ID a specific subscription in the ECP.
				DataType: IdentifierType
				Content: String
				Examples: 002
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:IDType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="16" />
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="UpdateLEDStatusCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Status of a LED request to indicate if it has been
				processed or
				not.
				DataType: TextType
				Content: String
				Examples: Update
				done on Company Name: IKEA
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:StatusCodeType">
				<xsd:enumeration value="PROCESSED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update has been
							processed
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="IGNORED">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">The update has not
							been processed.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="URIType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Unique Resource Identifier of the external reference of
				the document
				DataType: IdentifierType
				Content: string
				Examples:
				https://www.iso.org/
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:URIType">
				<xsd:minLength value="1" />
				<xsd:maxLength value="2048" />
				<xsd:pattern value="https?://.+" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="WarningType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				ComponentType: Basic
				Definition: Description of an exception returned by SOLR if there is
				an error
				in processing a request for a simple search.
				DataType: CodeType
				Content: string
				Examples: WARN_ES_01
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cbc:ResponseCodeType">
				<xsd:pattern value=".*[\S].*" />
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
