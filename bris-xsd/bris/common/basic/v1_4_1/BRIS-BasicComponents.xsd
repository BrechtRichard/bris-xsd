			<xsd:enumeration value="it">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Italian
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="lt">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Lithuanian
