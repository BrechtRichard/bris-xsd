<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema
	xmlns="http://ec.europa.eu/bris/v1_5/common/AggregateComponents/BusinessRegisterReference"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:bbc="http://ec.europa.eu/bris/v1_4/common/BasicComponents"
	targetNamespace="http://ec.europa.eu/bris/v1_5/common/AggregateComponents/BusinessRegisterReference"
	elementFormDefault="qualified"
	attributeFormDefault="unqualified"
	version="0">

    <xsd:import
            namespace="http://ec.europa.eu/bris/v1_4/common/BasicComponents"/>

	<xsd:element name="BusinessRegisterReference">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="bbc:BusinessRegisterCode" />
                <xsd:element ref="bbc:BusinessRegisterCountry" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

</xsd:schema>
